{
    "version": 2,
    "name": "foody-travelers",
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
        ]
      }
    ]
  }